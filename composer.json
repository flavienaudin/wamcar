{
  "name": "wamcar/wamcar",
  "description" : "Plateforme Wamcar",
  "license": "proprietary",
  "type": "project",
  "autoload": {
    "psr-4": {
      "": "src/",
      "Application\\Migrations\\": "database/migrations"
    },
    "classmap": [
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Behavior\\Context\\": "tests/behavior/bootstrap",
      "Test\\Unit\\": "tests/unit",
      "Application\\Fixtures\\": "database/fixtures"
    }
  },
  "require": {
    "php": "^7.2",
    "beberlei/doctrineextensions": "^1.3",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/doctrine-migrations-bundle": "^1.2",
    "doctrine/orm": "^2.7",
    "easycorp/easyadmin-bundle": "^1.0",
    "friendsofsymfony/ckeditor-bundle": "^2.1",
    "google/apiclient": "^2.0",
    "hwi/oauth-bundle": "^1.2.0",
    "incenteev/composer-parameter-handler": "^2.0",
    "jmose/command-scheduler-bundle": "^2.0",
    "league/csv": "^9.0",
    "liip/imagine-bundle": "^1.9",
    "mgilet/notification-bundle": "^3.0",
    "myclabs/php-enum": "^1.5",
    "novaway/elasticsearch-client": "^6.2",
    "php-http/guzzle6-adapter": "^2.0",
    "presta/sitemap-bundle": "^1.5",
    "ramsey/uuid-doctrine": "^1.4",
    "robertotru/to-inline-style-email-bundle": "^1.5",
    "ruflin/elastica": "^6.1",
    "sensio/distribution-bundle": "^5.0.19",
    "sensio/framework-extra-bundle": "^5.1",
    "simple-bus/symfony-bridge": "^5.0",
    "stof/doctrine-extensions-bundle": "^1.2",
    "symfony/monolog-bundle": "^3.1.0",
    "symfony/polyfill-apcu": "^1.0",
    "symfony/swiftmailer-bundle": "^3.1.6",
    "symfony/symfony": "^3.4",
    "symfony/translation": "^3.3",
    "twig/extensions": "^1.5",
    "vich/uploader-bundle": "^1.6"
  },
  "require-dev": {
    "sensio/generator-bundle": "^3.0",
    "novaway/common-contexts": "~2.0",
    "doctrine/doctrine-fixtures-bundle": "^2.4"
  },
  "scripts": {
    "symfony-scripts": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-install-cmd": [
      "@symfony-scripts"
    ],
    "post-update-cmd": [
      "@symfony-scripts"
    ]
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "web",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    },
    "branch-alias": null
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  }
}

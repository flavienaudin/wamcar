{
  "name": "novaproject/wamcar",
  "license": "proprietary",
  "type": "project",
  "autoload": {
    "psr-4": {
      "": "src/",
      "Application\\Migrations\\": "database/migrations"
    },
    "classmap": [
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Behavior\\Context\\": "tests/behavior/bootstrap",
      "Test\\Unit\\": "tests/unit",
      "Application\\Fixtures\\": "database/fixtures"
    }
  },
  "require": {
    "php": "^7.1.1",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/orm": "2.5.*",
    "incenteev/composer-parameter-handler": "^2.0",
    "sensio/distribution-bundle": "^5.0.19",
    "sensio/framework-extra-bundle": "^3.0.2",
    "symfony/monolog-bundle": "^3.1.0",
    "symfony/polyfill-apcu": "^1.0",
    "symfony/swiftmailer-bundle": "^3.1.6",
    "symfony/symfony": "3.4.*",
    "twig/twig": "^1.0||^2.0",
    "doctrine/doctrine-migrations-bundle": "^1.2",
    "stof/doctrine-extensions-bundle": "^1.2",
    "myclabs/php-enum": "^1.5",
    "ramsey/uuid-doctrine": "^1.4",
    "vich/uploader-bundle": "^1.6",
    "symfony/translation": "^3.3",
    "novaway/elasticsearch-client": "2.3.x-dev",
    "league/csv": "^9.0",
    "simple-bus/symfony-bridge": "^5.0",
    "guzzlehttp/guzzle": "^6.3",
    "zircote/swagger-php": "^2.0",
    "pragmarx/zipcode": "^1.5",
    "twig/extensions": "^1.5",
    "liip/imagine-bundle": "^1.9",
    "robertotru/to-inline-style-email-bundle": "^1.5",
    "hwi/oauth-bundle":"^0.6",
    "php-http/guzzle6-adapter":"^1.1",
    "ocramius/proxy-manager": "2.1"
  },
  "require-dev": {
    "sensio/generator-bundle": "^3.0",
    "atoum/atoum": "^3.0@dev",
    "atoum/stubs": "^2.5",
    "behat/behat": "^3.3",
    "behat/mink-extension": "^2.2",
    "behat/mink-goutte-driver": "^1.2",
    "behat/mink-selenium2-driver": "^1.3",
    "behat/symfony2-extension": "^2.1",
    "novaway/common-contexts": "~2.0",
    "doctrine/doctrine-fixtures-bundle": "^2.4",
    "deployer/deployer": "^6.0",
    "deployer/recipes": "^6.0"
  },
  "scripts": {
    "symfony-scripts": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-install-cmd": [
      "@symfony-scripts"
    ],
    "post-update-cmd": [
      "@symfony-scripts"
    ]
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "web",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    },
    "branch-alias": null
  },
  "config": {
    "bin-dir": "bin"
  }
}

{% set balise = balise|default('h2') %}

<article class="vehicle-item is-flex">
    {{ include('front/Search/includes/vehicle_item.html.twig') }}

    <a href="{{ vehicle.sellerUrl }}">
        <div class="vehicle-user-infos">
            <div class="vehicle-user-pic"><img src="{{ vehicle.sellerPicture }}" alt="{{ vehicle.sellerName }}"></div>
            <div class="small-12 dark-gray-color">
                <p class="no-margin"><strong class="primary-color">{{ vehicle.sellerName }}</strong></p>
                <a href="{{ vehicle.garageUrl }}" class="dark-gray-color">{{ vehicle.garageName }}</a>
            </div>
        </div>
    </a>
</article>

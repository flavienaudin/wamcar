{% extends 'front/Layout/layout.html.twig' %}

{% block mainClass %}no-padding-top light-gray-peexeo-bg{% endblock %}

{% block title %}{{ 'user.proservices.edit.title'|trans }}{% endblock %}

{% set breadcrumbs = [
    {
        'name': 'user.profile.my_profile'|trans,
        'href': path('front_view_current_user_info')
    },
    {
        'name': block('title'),
        'href': app.request.requestUri
    }
] %}

{% block body %}
    {% include 'front/Layout/includes/breadcrumb.html.twig' %}
    {% include 'front/Layout/includes/page_header.html.twig' with {
        title: block('title'),
        page_header_class: 'white-bg'
    } %}

    {% include "front/Seller/includes/form_edit_proservices.html.twig" %}
{% endblock %}
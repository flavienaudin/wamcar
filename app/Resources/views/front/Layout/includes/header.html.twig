<header class="l-header column small-right-collapse large-right-uncollapse">
     {% include "front/Layout/includes/logo.html.twig" %}
     {% block headerCenter %}
          {% include "front/Layout/includes/navigation/primary.html.twig" %}
     {% endblock %}
     <div class="l-header-right block-grid column small-collapse small-right medium-3 large-uncollapse">
          {% block headerRight %}
               <button type="button" class="l-header-button icon-user small-right" data-toggle="js-off-canvas-user"><span class="show-for-large">Espace membre</span></button>
               <button class="l-header-button icon-hamburger primary-color hide-for-large" data-toggle="js-off-canvas-navigation"><span class="show-for-sr">Navigation</span></button>
          {% endblock %}
     </div>
</header>

<div id="js-off-canvas-user" class="off-canvas position-right" data-off-canvas>
     {% include 'front/Layout/includes/off_canvas_user.html.twig' %}
</div>
<div id="js-off-canvas-navigation" class="off-canvas position-right" data-off-canvas></div>
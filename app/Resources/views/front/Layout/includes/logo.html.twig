{% set isLogged = isLogged|default(false) %}
{% set action = isLogged ? path('front_directory_view') : path('front_default') %}

<a href="{{ action }}" class="logo-container column shrink {{ isLogged ? 'small-collapse large-uncollapse is-logged' }}">
    <img src="{{ asset('assets/images/logo/w_denicheur.svg') }}" alt="Wamcar" class="logo show-for-small-only">
    <img src="{{ asset('assets/images/logo/wamcar_denicheur.svg') }}" alt="Wamcar" class="logo hide-for-small-only">
</a>

{% block _vehicle_pictures_entry_widget %}
    {% set class = class|default('') %}
    {% set image = value ? value.realPicture|vehiclePicture('vehicle_thumbnail') : asset('vehicle_thumbnail'|defaultVehiclePicture) %}
    {% set isEdit = value and value.realPicture ? true : false %}
    {% set title = title|default('Photo supplémentaire conseillée') %}

    <div id="js-file-preview-{{ form.vars.id }}" class="js-file-index block-grid small-12 medium-6 {{ class }}">
        <label for="{{ form.vars.id }}" class="js-file-preview-label file-preview-label column small-12">{{ title }} <span class="medium-12 medium-no-margin">Facultatif (format conseillé : 310x235)</span></label>
        <div class="js-file-preview file-preview column small-6 medium-12">
            {{ form_widget(form.id) }}
            {{ form_widget(form.file, { 'attr' : { 'class' : 'js-file-preview-input is-hidden' } }) }}
            {{ form_widget(form.isRemoved, { 'attr' : { 'class' : 'js-file-remove-input is-hidden' } }) }}
            <label for="{{ form.vars.id }}_file" title="Ajouter une photo" class="js-file-preview-image-container file-preview-image-container">
                <div class="file-preview-image no-margin">
                    <span><img src="{{ asset(image) }}" width="100%" class="js-file-preview-image"></span>
                </div>
                <span class="show-for-sr">Ajouter une photo</span>
            </label>
            <button type="button" class="js-file-preview-remove file-preview-remove icon-remove {% if not isEdit %}is-hidden{% endif %}" title="Supprimer la photo"><span class="show-for-sr">Supprimer la photo</span></button>
        </div>
        <div class="column small-6 medium-12">
            {{ form_widget(form.caption, { 'attr' : { 'class' : 'js-file-legend input small', 'placeholder' : 'Ajouter une légende' } } ) }}
        </div>
    </div>
{% endblock %}


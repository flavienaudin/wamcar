{% extends 'front/Directory/includes/seller_item.html.twig' %}

{% block bottomContainer %}
    <div class="js-seller-infos peexeo-vehicle-seller-infos">
        {% if proUser.phone is not empty or proUser.phonePro is not empty %}
            <div class="is-flex">
            {{ include('front/Seller/includes/phone_number_button.html.twig', {
                'disabledLoggedProtection': true,
                'phoneOwner': proUser,
                'phoneNumber': proUser.phone is not empty ? proUser.phone : proUser.phonePro ,
                'showphone_subaction': proUser.phone is not empty ? "1" : "2"
            }) }}
            </div>
        {% endif %}
    </div>
{% endblock bottomContainer %}
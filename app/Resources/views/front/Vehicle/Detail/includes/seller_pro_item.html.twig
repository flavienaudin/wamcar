{% extends 'front/User/includes/seller_pro_item.html.twig' %}

{% block bottomContainer %}
    <div class="js-seller-infos peexeo-vehicle-seller-infos">
        {% if seller.phone is not empty or seller.phonePro is not empty %}
            <div class="is-flex">
            {{ include('front/Seller/includes/phone_number_button.html.twig', {
                'disabledLoggedProtection': true,
                'phoneOwner': seller,
                'phoneNumber': seller.phone is not empty ? seller.phone : seller.phonePro ,
                'showphone_subaction': seller.phone is not empty ? "1" : "2"
            }) }}
            </div>
        {% endif %}
    </div>
{% endblock bottomContainer %}
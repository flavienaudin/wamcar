
<ul class="brand-list list-border block-border">
    <li class="no-margin">
        <figure class="block-grid align-middle" role="group">
            <span class="brand-logo {{ info.tel is defined ? 'is-logo' : '' }}"><img src="{{ asset('assets/images/placeholders/garages/garage-360x160.jpg') }}" alt="{{ vehicle.garage.name }}"></span>
            <div>
                <a href="{{ path('front_garage_view', {'id': vehicle.garage.id}) }}" class="primary-color h3"><b>{{ vehicle.garage.name }}</b></a> <br>
                <span>{{ vehicle.garage.address.cityName }}</span>
            </div>
        </figure>
    </li>
    {% if vehicle.seller.name %}
    <li class="no-margin">
        <figure class="block-grid align-middle" role="group">
            <span class="brand-logo is-logo"><img src="{{ asset('assets/images/placeholders/user/user.png') }}" alt="{{ vehicle.seller.name }}"></span>
            <div>
                <a href="{{ path('front_view_user_info', {'id': vehicle.garage.seller.id}) }}" class="primary-color h3"><b>{{ vehicle.seller.name }}</b></a> <br>
                <span class="icon-tel primary-color">
                    <a href="tel:{{ vehicle.seller.phonePro }}" class="text-underline" target="_blank" rel="noopener noreferrer">{{ vehicle.seller.phonePro }}</a>
                </span>
            </div>
        </figure>
    </li>
    {% endif %}
</ul>
